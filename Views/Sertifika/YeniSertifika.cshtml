@model MvcCv.Models.Entity.Tbl_serfikalarım
@{
    ViewBag.Title = "YeniSertifika";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<h4>Yeni Sertifika Ekleme Sayfası</h4>
<form method="post">

    @Html.Label("Sertifika Adı")
    @Html.TextBoxFor(x => x.Aciklama, new { @class = "form-control" })
    <br />
    @Html.Label("Tarih")
    @Html.TextBoxFor(x => x.Tarih, new { @class = "form-control" })
    <br />
    <button class="btn btn-success">Kaydet</button>

</form>


